{
  "mcpServers": {
    "example-stdio-server": {
      "command": "uvx",
      "args": ["mcp-server-package"],
      "env": {
        "API_KEY": "your-api-key-here"
      }
    },
    "example-sse-server": {
      "transport": "sse",
      "url": "http://localhost:8000/sse"
    },
    "example-http-server": {
      "transport": "streamable-http",
      "url": "http://localhost:8001/mcp"
    }
  }
}
